package practice5;

public class MissingNumber {
    public static void main(String[] args) {
        int arr [] = {1,2,4,5,6,8};
        int  x = 1;
        System.out.print("missing number is : ");
        for(int i =0 ; i<arr.length-1;i++){
            if(arr[i+1]!=arr[i]+1){
                System.out.print(arr[i]+1+",");
            }
        }
    }
}
